     A                                      WDWTITLE((*TEXT 'CONFIRM (Y/N)')
     A                                               (*COLOR WHT)
     A                                               *CENTER)
     A                                      RTNCSRLOC(*RECNAME &D__CURSREC
     A                                                &D__CURSFLD &D__CURSPOS)
     A  27                                  CSRLOC(D__CURSROW D__CURSCOL)
